{
  "id": "/rest/pagination_request",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Pagination request",
  "type": "object",
  "properties":{
    "start": {
      "type": "int",
      "description": "Start of this page.  Where exactly start is depends on the context."
    },
    "count": {
      "type": "int",
      "description": "Number of elements after start."
    }
  }
}